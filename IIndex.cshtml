        }

        .acknowledge input[type="submit"], .acknowledge a {
            margin-left: 183px;
            background: #253673;
            border: 0px;
            color: #f4cc2d;
            padding: 5px 10px;
            font-size: 14px;
        }

            .acknowledge input[type="submit"]:hover, .acknowledge a:hover {
                background: #3e3e3e;
                text-decoration: none;
            }

        .label {
            text-align: right;
            width: 200px;
            padding-right: 10px;
        }

        .notice {
            background: #ffffff;
            padding: 20px 40px;
        }

            .notice p {
                text-align: justify;
                font-size: 16px;
                color: #666666;
            }

                .notice p i {
                    color: #000000;
                }
    </style>

</head>

<body>
    
